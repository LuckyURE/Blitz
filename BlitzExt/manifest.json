{
	 "manifestVersion": 1,
	 "id": "blitz-allpulls-extension",
	 "version": "0.3.5",
	 "name": "Sierpinski's Blitz Active Pull Requests",
	 "description": "An extension that shows all active pull requests.",
	 "publisher": "sierpinski",
	 "icons": {
        "default": "images/blitzallpulls.png"
		},
	"categories": [
        "Code"
    ],
	"tags": [
        "pull request",
		"git"
    ],
	"galleryFlags": [
        "Public",
		"Preview"
    ],
	"content": {
        "details": {
            "path": "overview.md"
        },
        "license": {
            "path": "license-terms.md"
        }
    },
	"screenshots": [
		{
            "path": "images/ScreenCaptureExtv03.png"
        },
        {
            "path": "images/ScreenCapNav.png"
        }
    ],
	"links": {
        "home": {
            "uri": "https://github.com/sierpinski/Blitz"
        }
    },
    "branding": {
        "color": "rgb(90, 86, 206)",
        "theme": "dark"
    },
	 "targets": [
			 {
				 "id": "Microsoft.VisualStudio.Services"
			 }
		 ],
	 "contributions": [
			 {
				 "id": "Sierpinski.BlitzActivePulls",
				 "type": "ms.vss-web.hub",
				 "description": "Shows all active pull requests.",
				 "targets": [
					 "ms.vss-code-web.code-hub-group"
				 ],
				 "properties": {
					 "name": "Active Pull Requests",
					 "order": 99,
					 "uri": "blitzallpulls.html"
				 }
			 }
		 ],
     "scopes": [
			 "vso.code"
		 ],
	"files": [
         {
             "path": "blitzallpulls.html", "addressable": true
         },
         {
             "path": "sdk/scripts", "addressable": true
         }
	]
}